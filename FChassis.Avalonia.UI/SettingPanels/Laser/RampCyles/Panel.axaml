<base:Panel
	    xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FChassis.UI.Settings.Laser.RampCycles.Panel"
		xmlns:base="FChassis.UI.Settings"
		>
	<Grid RowDefinitions="40, *" ColumnDefinitions="300, *">
		<Panel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
			<StackPanel Orientation="Horizontal" Margin="4">
				<TextBlock Text="Cycle" VerticalAlignment="Center"/>
				<ComboBox/>
				<Button Content="+" Margin="3"/>
				<Button Content="-" Margin="3"/>
				<Button Content="P" Margin="3"/>
				<TextBlock Text="Gating Frequency :" VerticalAlignment="Center"/>
				<TextBox Text="100 Hz"/>
				<TextBlock Text="Pulser Power :" VerticalAlignment="Center"/>
				<TextBox Text="300 W"/>
			</StackPanel>
		</Panel>
		<Panel Background="#26364C"
			   Grid.Row="1" Grid.Column="0"/>
		<Panel Grid.Row="1" Grid.Column="1"/>
	</Grid>
</base:Panel>
