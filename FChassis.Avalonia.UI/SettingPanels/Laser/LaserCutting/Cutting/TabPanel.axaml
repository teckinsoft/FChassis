<base:Panel
	    xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		d:DesignWidth="800" 
		xmlns:base="FChassis.UI.Settings"
        x:Class="FChassis.UI.Settings.Laser.LaserCutting.Cutting.TabPanel"
		xmlns:cp="clr-namespace:FChassis.UI.Settings.Laser.LaserCutting"
		>
	<Grid x:Name="Grid" RowDefinitions="32, *">
		<cp:CommonPanel/>
		<TabControl x:Name="TabControl" Margin="5"
				Background="AliceBlue" Grid.Row="1"
				TabStripPlacement="Left">
			<TabItem Header="Large"/>
			<TabItem Header="Medium"/>
			<TabItem Header="Small"/>
			<TabItem Header="Special"/>
			<TabItem Header="Prehole"/>
		</TabControl>
	</Grid>
</base:Panel>
